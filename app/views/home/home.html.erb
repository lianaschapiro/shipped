<h1>Welcome to Shipped</h1>

<h2>Please Log In:</h2>
<%= form_tag log_in_path do %>
<h3><%= label_tag(:username) %></h3>
<%= text_field_tag(:username) %>
<h3><%= label_tag(:password) %></h3>
<%= password_field_tag(:password) %>
<br><br>
<%= submit_tag("Log In") %>
<% end %>

<h3>Or <%= link_to "sign up here", new_user_path %> </h3>
